#include <iostream>
#include <cmath>

int main() {
    double x, y;

    std::cout << "Введите значение x: ";
    std::cin >> x;

    if (x > 0) {
        y = sin(x) * sin(x);
    } else {
        y = 1 + 2 * sin(x) * sin(x);
    }

    std::cout << "Значение y = " << y << std::endl;

    return 0;
}
